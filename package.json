{
    "name": "purduetemplates2015",
    "version": "1.6.7",
    "description": "The Purdue University templates created in 2015 by Purdue Marketing and Media.",
    "main": "gulpfile.js",
    "scripts": {
        "html": "NODE_ENV=development node html-render.js",
        "scss:dev": "NODE_ENV=development node styles-render.js",
        "js:dev": "run-p js:dev:*",
        "js:dev:templates": "rm -rf new_builds/js/mm/templates && mkdir -p new_builds/js/mm/templates && webpack src/js/mm/main.js -o new_builds/js/mm/templates/main.js && browser-sync reload",
        "js:prod:templates": "rm -rf new_builds/js/mm/templates && mkdir -p new_builds/js/mm/templates && webpack -p src/js/mm/main.js -o new_builds/js/mm/templates/main.js && browser-sync reload",
        "serve": "browser-sync new_builds --directory -w -b Firefox",
        "preserve": "run-p html scss:dev js:dev",
        "watch": "run-p watch:*",
        "watch:scss": "onchange 'src/**/*.scss' -- npm run scss:dev",
        "watch:js": "onchange 'src/**/*.js' -- npm run js:dev",
        "watch:json": "onchange 'src/**/*.json' -- npm run html",
        "watch:handlebars": "onchange 'src/**/*.handlebars' -- npm run html",
        "dev": "run-p serve watch",
        "prod": ""
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/PurdueMarketingAndMedia/purdueTemplates-2015.git"
    },
    "author": "Purdue Office of Marketing and Media",
    "license": "No License",
    "bugs": {
        "url": "https://github.com/PurdueMarketingAndMedia/purdueTemplates-2015/issues"
    },
    "homepage": "https://github.com/PurdueMarketingAndMedia/purdueTemplates-2015#readme",
    "devDependencies": {
        "@babel/core": "^7.4.3",
        "@babel/preset-env": "^7.4.3",
        "async": "^2.1.5",
        "autoprefixer": "^9.4.7",
        "babel-loader": "^8.0.5",
        "babel-preset-minify": "^0.5.0",
        "browser-sync": "^2.26.7",
        "del": "^3.0.0",
        "gulp": "^3.9.1",
        "gulp-connect": "^5.7.0",
        "gulp-file-include": "^2.0.1",
        "gulp-html-prettify": "0.0.1",
        "gulp-postcss": "^7.0.1",
        "gulp-sass": "^4.0.2",
        "gulp-sourcemaps": "^2.4.0",
        "gulp-zip": "^4.1.0",
        "npm-run-all": "^4.1.5",
        "onchange": "^5.2.0",
        "postcss": "^7.0.14",
        "postcss-pseudoelements": "^5.0.0",
        "webpack": "^4.30.0",
        "webpack-cli": "^3.3.0"
    },
    "browserslist": [
        "last 2 versions",
        "> 5% in my stats"
    ],
    "dependencies": {
        "handlebars": "^4.1.2",
        "node-sass": "^4.11.0",
        "pretty": "^2.0.0"
    }
}
